package de.ovgu.dke.util;


public class SimpleObjectWrapper extends ObjectWrapper 
{
	public SimpleObjectWrapper(double[] representation)
	{
		this(null, representation);
	}
	
	public SimpleObjectWrapper(double[] representation, String classLabel)
	{
		this(null, representation);
		this.classLabel = classLabel;
	}
	
	
	public SimpleObjectWrapper(Object object, double[] representation) 
	{
		super(object, representation);
	}

}
